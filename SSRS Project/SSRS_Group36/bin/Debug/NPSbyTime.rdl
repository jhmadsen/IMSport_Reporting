  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>IMSportsSalesDW</DataSourceName>
        <CommandText>SELECT        DimTime.timYear, DimTime.timMonth, ROUND(SUM(CASE WHEN CustomerSatisfaction &gt;= 9 THEN CAST(1 AS float) ELSE CAST(0 AS float) END) / COUNT(FactSalesHeader.CustomerSatisfaction) 
                         - SUM(CASE WHEN CustomerSatisfaction &lt;= 6 THEN CAST(1 AS float) ELSE CAST(0 AS float) END) / COUNT(FactSalesHeader.CustomerSatisfaction),2) AS NPS
FROM            FactSalesHeader INNER JOIN
                         DimTime ON DimTime.SK_TimeID = FactSalesHeader.FK_TimeID
GROUP BY DimTime.timYear, DimTime.timMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="timYear">
          <DataField>timYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="timMonth">
          <DataField>timMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NPS">
          <DataField>NPS</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
