  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>IMSportsSalesDW</DataSourceName>
        <CommandText>SELECT        DimTime.timYear, DimSalesPerson.salName, ROUND(SUM(CASE WHEN CustomerSatisfaction &gt;= 9 THEN CAST(1 AS float) ELSE CAST(0 AS float) END) / COUNT(FactSalesHeader.CustomerSatisfaction) 
                         - SUM(CASE WHEN CustomerSatisfaction &lt;= 6 THEN CAST(1 AS float) ELSE CAST(0 AS float) END) / COUNT(FactSalesHeader.CustomerSatisfaction),2) AS NPS, COUNT(FactSalesHeader.CustomerSatisfaction) 
                         AS CountOfSales
FROM            FactSalesHeader INNER JOIN
                         DimTime ON DimTime.SK_TimeID = FactSalesHeader.FK_TimeID INNER JOIN
                         DimSalesPerson ON DimSalesPerson.SK_SalesPersonID = FactSalesHeader.FK_SalesPersonID
GROUP BY DimTime.timYear, DimSalesPerson.salName</CommandText>
      </Query>
      <Fields>
        <Field Name="timYear">
          <DataField>timYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="salName">
          <DataField>salName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NPS">
          <DataField>NPS</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="CountOfSales">
          <DataField>CountOfSales</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
